#: modules/video_filter/marq.c:107 modules/video_filter/mosaic.c:170
#: modules/video_filter/osdmenu.c:78 modules/video_filter/rss.c:160
#: modules/video_filter/time.c:99
msgid "Bottom"
msgstr "底部"

