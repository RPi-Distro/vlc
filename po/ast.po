"  file:///path/file              Plain media file\n"
"  http://host[:port]/file        HTTP URL\n"
"  ftp://host[:port]/file         FTP URL\n"
"  mms://host[:port]/file         MMS URL\n"
"  screen://                      Screen capture\n"
"  dvd://[device]                 DVD device\n"
