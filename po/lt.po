msgid "Best available"
msgstr "Geriausia galima"

#: src/libvlc-module.c:768
msgid "Full HD (1080p)"
msgstr ""

#: src/libvlc-module.c:768
msgid "HD (720p)"
msgstr "HD (720p)"

#: src/libvlc-module.c:769
