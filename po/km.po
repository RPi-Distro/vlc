msgid "Blend"
msgstr "ល្បាយ"

#: src/libvlc-module.c:510 modules/video_filter/deinterlace/deinterlace.h:57
msgid "Mean"
msgstr "មធ្យម"

#: src/libvlc-module.c:510 modules/video_filter/deinterlace/deinterlace.h:57
msgid "Bob"
msgstr "ប៉ប"

#: src/libvlc-module.c:511 modules/video_filter/deinterlace/deinterlace.h:57
