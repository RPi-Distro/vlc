#: modules/gui/wxwidgets/dialogs/vlm/vlm_panel.cpp:67
msgid "VOD"
msgstr "ویدئوی درخواستی"

#: include/vlc_config_cat.h:178
msgid "VLC's implementation of Video On Demand"
