msgstr "PMT PID"

#: modules/mux/mpeg/ts.c:106
msgid "Assign a fixed PID to the PMT"
msgstr "PMTに固定のPIDを割り当てます。"

#: modules/mux/mpeg/ts.c:107
msgid "TS ID"
msgstr "TS ID"

#: modules/mux/mpeg/ts.c:108
